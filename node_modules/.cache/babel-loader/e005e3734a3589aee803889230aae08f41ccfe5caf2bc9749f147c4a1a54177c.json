{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nimport data from \"@/data/data.json\";\nimport Arrow from \"../components/Arrow.vue\";\nimport \"@fortawesome/fontawesome-free/css/all.css\";\nexport default {\n  name: \"Profile\",\n  components: {\n    Arrow\n  },\n  data() {\n    return {\n      sections: data.profile.sections,\n      heading: \"Languages and Awards\",\n      currentTab: data.profile.defaultTab,\n      fallbackImage: require(\"@/assets/images/fallback.png\")\n    };\n  },\n  computed: {\n    filteredItems() {\n      return this.sections.find(section => section.key === this.currentTab)?.items || [];\n    }\n  },\n  methods: {\n    setTab(tabKey) {\n      this.currentTab = tabKey;\n    },\n    getImageUrl(imageName) {\n      try {\n        return require(`@/assets/images/profile/${imageName}`);\n      } catch (e) {\n        return this.fallbackImage;\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","Arrow","name","components","sections","profile","heading","currentTab","defaultTab","fallbackImage","require","computed","filteredItems","find","section","key","items","methods","setTab","tabKey","getImageUrl","imageName","e"],"sources":["src/components/Portfolio.vue"],"sourcesContent":["<template>\n  <section id=\"profile\" class=\"dark-section\">\n    <div class=\"container-fluid\">\n      <h1 class=\"section-header\">{{ heading }}</h1>\n\n      <!-- Tab Navigation -->\n      <div class=\"row filters\">\n        <ul class=\"list-inline mx-auto\">\n          <li\n            v-for=\"tab in sections\"\n            :key=\"tab.key\"\n            class=\"list-inline-item filter\"\n          >\n            <a\n              class=\"nav-item\"\n              :class=\"tab.key === currentTab ? 'active' : null\"\n              :data-tab=\"tab.key\"\n              @click=\"setTab(tab.key)\"\n            >\n              {{ tab.title }}\n            </a>\n          </li>\n        </ul>\n      </div>\n\n      <!-- Content Display -->\n      <div id=\"profile-container\" class=\"row\">\n        <div\n          v-for=\"(item, index) in filteredItems\"\n          :key=\"index\"\n          class=\"col-sm-12 col-md-4 flex-col\"\n        >\n          <div class=\"portfolio-item\">\n            <!-- Language Flags -->\n            <template v-if=\"currentTab === 'languages'\">\n              <span class=\"flag-icon-awards\" :class=\"'flag-icon-' + item.flag\"></span>\n            </template>\n            <!-- Awards & Other Activities Images -->\n            <template v-else>\n              <a v-if=\"item.link\" :href=\"item.link\" target=\"_blank\">\n                <img\n                  :src=\"getImageUrl(item.image)\"\n                  :alt=\"item.title\"\n                  class=\"item-image\"\n                />\n              </a>\n              <img\n                v-else\n                :src=\"getImageUrl(item.image)\"\n                :alt=\"item.title\"\n                class=\"item-image\"\n              />\n            </template>\n\n            <!-- Overlay with Title and Description -->\n            <div class=\"overlay\">\n              <a :href=\"item.link\" title=\"Live Demo\" target=\"_blank\">\n              <div class=\"portfolio-item-content\">\n                <h3>{{ item.title }}</h3>\n                <p>{{ item.description }}</p>\n                \n                    <a :href=\"item.source\" title=\"Source Code\" target=\"_blank\">\n                      <i class=\"fas fa-code fa-2x\"></i>\n                    </a>\n                  <\n              </div>\n              </a>\n            </div>\n          \n          </div>\n        </div>\n      </div>\n    </div>\n    <Arrow />\n  </section>\n</template>\n\n<script>\nimport data from \"@/data/data.json\";\nimport Arrow from \"../components/Arrow.vue\";\nimport \"@fortawesome/fontawesome-free/css/all.css\";\n\nexport default {\n  name: \"Profile\",\n  components: {\n    Arrow,\n  },\n  data() {\n    return {\n      sections: data.profile.sections,\n      heading: \"Languages and Awards\",\n      currentTab: data.profile.defaultTab,\n      fallbackImage: require(\"@/assets/images/fallback.png\")\n    };\n  },\n  computed: {\n    filteredItems() {\n      return this.sections.find(section => section.key === this.currentTab)?.items || [];\n    }\n  },\n  methods: {\n    setTab(tabKey) {\n      this.currentTab = tabKey;\n    },\n    getImageUrl(imageName) {\n      try {\n        return require(`@/assets/images/profile/${imageName}`);\n      } catch (e) {\n        return this.fallbackImage;\n      }\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\">\n/* Style for the tab navigation */\n.filters {\n  text-align: center;\n  margin-bottom: 20px;\n}\n\n.filters .filter a {\n  padding: 10px 20px;\n  cursor: pointer;\n  color: white;\n  text-decoration: none;\n}\n\n.filters .filter .active {\n  background: #007bff;\n  border-radius: 5px;\n  color: white;\n}\n\n/* Style for the portfolio items */\n.portfolio-item {\n  position: relative;\n  overflow: hidden;\n  text-align: center;\n}\n\n.portfolio-item img {\n  width: 100%;\n  height: auto;\n  border-radius: 10px;\n}\n\n.overlay {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background: rgba(0, 0, 0, 0.7);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  opacity: 0;\n  transition: opacity 0.3s;\n}\n\n.portfolio-item:hover .overlay {\n  opacity: 1;\n}\n\n.portfolio-item-content {\n  color: white;\n  text-align: center;\n  padding: 15px;\n}\n\n.portfolio-item-content h3 {\n  margin-bottom: 10px;\n}\n\n/* Flag Styling */\n.flag-icon-awards {\n  display: inline-block;\n  width: 120px; /* Adjust as needed */\n  height: 80px; /* Adjust as needed */\n  font-size: 80px; /* Increases icon size */\n  background-size: cover;\n  background-position: center;\n  border-radius: 5px; /* Optional for slight rounding */\n  margin-bottom: 10px;\n}\n</style>\n"],"mappings":";;AA8EA,OAAAA,IAAA;AACA,OAAAC,KAAA;AACA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF;EACA;EACAD,KAAA;IACA;MACAI,QAAA,EAAAJ,IAAA,CAAAK,OAAA,CAAAD,QAAA;MACAE,OAAA;MACAC,UAAA,EAAAP,IAAA,CAAAK,OAAA,CAAAG,UAAA;MACAC,aAAA,EAAAC,OAAA;IACA;EACA;EACAC,QAAA;IACAC,cAAA;MACA,YAAAR,QAAA,CAAAS,IAAA,CAAAC,OAAA,IAAAA,OAAA,CAAAC,GAAA,UAAAR,UAAA,GAAAS,KAAA;IACA;EACA;EACAC,OAAA;IACAC,OAAAC,MAAA;MACA,KAAAZ,UAAA,GAAAY,MAAA;IACA;IACAC,YAAAC,SAAA;MACA;QACA,OAAAX,OAAA,4BAAAW,SAAA;MACA,SAAAC,CAAA;QACA,YAAAb,aAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
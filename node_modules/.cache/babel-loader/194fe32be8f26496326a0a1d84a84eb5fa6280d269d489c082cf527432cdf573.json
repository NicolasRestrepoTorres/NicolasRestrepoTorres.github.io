{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport data from \"@/data/data.json\";\nimport Arrow from \"../components/Arrow.vue\";\nexport default {\n  name: \"Skills\",\n  components: {\n    Arrow\n  },\n  data() {\n    return {\n      skills: data.skills.categories,\n      heading: data.main.headings.skills,\n      currentCategory: data.skills.defaultCategory\n    };\n  },\n  computed: {\n    filteredSkills() {\n      return this.skills.filter(skill => skill.category === this.currentCategory);\n    }\n  },\n  methods: {\n    setCategory(event) {\n      this.currentCategory = event.target.dataset.category;\n    }\n  }\n};","map":{"version":3,"names":["data","Arrow","name","components","skills","categories","heading","main","headings","currentCategory","defaultCategory","computed","filteredSkills","filter","skill","category","methods","setCategory","event","target","dataset"],"sources":["src/components/Skills.vue"],"sourcesContent":["<template>\n  <section id=\"skills\" class=\"light-section\">\n    <div class=\"container-fluid\">\n      <h1 class=\"section-header\">{{ heading }}</h1>\n\n      <!-- Filters -->\n      <div class=\"row filters\">\n        <ul\n          class=\"list-inline mx-auto\"\n          data-aos=\"fade-right\"\n          data-aos-duration=\"1000\"\n        >\n          <li\n            v-for=\"item in skills\"\n            :key=\"item.category\"\n            class=\"list-inline-item filter\"\n          >\n            <a\n              class=\"nav-item\"\n              :class=\"{ active: item.category === currentCategory }\"\n              :data-category=\"item.category\"\n              @click=\"setCategory\"\n              >{{ item.category }}</a\n            >\n          </li>\n        </ul>\n      </div>\n\n      <!-- Skill Container -->\n      <div\n        id=\"skill-container\"\n        v-for=\"item in filteredSkills\"\n        :key=\"item.category\"\n        data-aos=\"fade-right\"\n        data-aos-duration=\"1000\"\n      >\n        <div class=\"category-heading\">\n          <h2><i :class=\"item.faClass\"></i> {{ item.category }}</h2>\n          <h3 v-if=\"item.subtitle\">{{ item.subtitle }}</h3>\n        </div>\n\n        <div class=\"skill-content\">\n          <!-- Skill Bars -->\n          <div v-if=\"item.skillList.length\" class=\"skillbars\">\n            <div\n              class=\"skill\"\n              v-for=\"skill in item.skillList\"\n              :key=\"skill.name\"\n            >\n              <h3 class=\"skill-name\">{{ skill.name }}</h3>\n              <div class=\"outer-bar\">\n                <div\n                  class=\"inner-bar\"\n                  :style=\"{ width: skill.level + '%' }\"\n                ></div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <Arrow />\n  </section>\n</template>\n\n<script>\nimport data from \"@/data/data.json\";\nimport Arrow from \"../components/Arrow.vue\";\n\nexport default {\n  name: \"Skills\",\n  components: {\n    Arrow,\n  },\n  data() {\n    return {\n      skills: data.skills.categories,\n      heading: data.main.headings.skills,\n      currentCategory: data.skills.defaultCategory,\n    };\n  },\n  computed: {\n    filteredSkills() {\n      return this.skills.filter(skill => skill.category === this.currentCategory);\n    },\n  },\n  methods: {\n    setCategory(event) {\n      this.currentCategory = event.target.dataset.category;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n/* Example SCSS for styling skill bars and filters */\n\n.skillbars {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 1rem;\n\n  .skill {\n    flex: 1;\n    min-width: 200px;\n\n    .skill-name {\n      margin-bottom: 0.5rem;\n      font-weight: bold;\n    }\n\n    .outer-bar {\n      background: #eaeaea;\n      border-radius: 5px;\n      height: 10px;\n      overflow: hidden;\n      position: relative;\n\n      .inner-bar {\n        height: 100%;\n        background: #007bff;\n        border-radius: 5px;\n        transition: width 0.3s ease;\n      }\n    }\n  }\n}\n\n.filters {\n  margin-bottom: 2rem;\n\n  .nav-item {\n    padding: 0.5rem 1rem;\n    cursor: pointer;\n    transition: color 0.3s ease;\n\n    &.active {\n      font-weight: bold;\n      color: #007bff;\n    }\n\n    &:hover {\n      color: #0056b3;\n    }\n  }\n}\n</style>\n"],"mappings":";;AAmEA,OAAAA,IAAA;AACA,OAAAC,KAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF;EACA;EACAD,KAAA;IACA;MACAI,MAAA,EAAAJ,IAAA,CAAAI,MAAA,CAAAC,UAAA;MACAC,OAAA,EAAAN,IAAA,CAAAO,IAAA,CAAAC,QAAA,CAAAJ,MAAA;MACAK,eAAA,EAAAT,IAAA,CAAAI,MAAA,CAAAM;IACA;EACA;EACAC,QAAA;IACAC,eAAA;MACA,YAAAR,MAAA,CAAAS,MAAA,CAAAC,KAAA,IAAAA,KAAA,CAAAC,QAAA,UAAAN,eAAA;IACA;EACA;EACAO,OAAA;IACAC,YAAAC,KAAA;MACA,KAAAT,eAAA,GAAAS,KAAA,CAAAC,MAAA,CAAAC,OAAA,CAAAL,QAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
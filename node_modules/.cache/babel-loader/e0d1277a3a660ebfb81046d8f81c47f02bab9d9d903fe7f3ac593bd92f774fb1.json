{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nimport data from \"@/data/data.json\";\nimport Arrow from \"../components/Arrow.vue\";\nexport default {\n  name: \"portfolio\",\n  components: {\n    Arrow\n  },\n  data() {\n    return {\n      categories: data.portfolio.categories,\n      heading: \"Portfolio\",\n      currentFilter: data.portfolio.defaultFilter\n    };\n  },\n  computed: {\n    filteredItems() {\n      const category = this.categories.find(cat => cat.key === this.currentFilter);\n      return category ? category.items : [];\n    }\n  },\n  methods: {\n    setFilter(event) {\n      this.currentFilter = event.target.dataset.filter;\n    }\n  }\n};","map":{"version":3,"names":["data","Arrow","name","components","categories","portfolio","heading","currentFilter","defaultFilter","computed","filteredItems","category","find","cat","key","items","methods","setFilter","event","target","dataset","filter"],"sources":["src/components/Portfolio.vue"],"sourcesContent":["<template>\n  <section id=\"portfolio\" class=\"dark-section\">\n    <div class=\"container-fluid\">\n      <h1 class=\"section-header\">{{ heading }}</h1>\n\n      <!-- Tabs for categories -->\n      <div class=\"row filters\">\n        <ul class=\"list-inline mx-auto\">\n          <li\n            v-for=\"category in categories\"\n            :key=\"category.key\"\n            class=\"list-inline-item filter\"\n          >\n            <a\n              class=\"nav-item\"\n              :class=\"category.key === currentFilter ? 'active' : null\"\n              :data-filter=\"category.key\"\n              @click=\"setFilter\"\n            >{{ category.title }}</a>\n          </li>\n        </ul>\n      </div>\n\n      <!-- Content Section -->\n      <div id=\"portfolio-container\" class=\"row\">\n        <div v-if=\"currentFilter === 'languages'\">\n          <ul>\n            <li v-for=\"(item, index) in filteredItems\" :key=\"index\">\n              <strong>{{ item.name }}</strong>: {{ item.level }}\n            </li>\n          </ul>\n        </div>\n\n        <div v-if=\"currentFilter === 'awards'\">\n          <div v-for=\"(item, index) in filteredItems\" :key=\"index\" class=\"award-card\">\n            <h3>{{ item.title }}</h3>\n            <p><strong>{{ item.date }}</strong></p>\n            <p>{{ item.description }}</p>\n          </div>\n        </div>\n\n        <div v-if=\"currentFilter === 'activities'\">\n          <div v-for=\"(item, index) in filteredItems\" :key=\"index\" class=\"activity-card\">\n            <h3>{{ item.title }}</h3>\n            <p>{{ item.description }}</p>\n            <a v-if=\"item.link\" :href=\"item.link\" target=\"_blank\">View</a>\n          </div>\n        </div>\n      </div>\n    </div>\n    <Arrow />\n  </section>\n</template>\n\n<script>\nimport data from \"@/data/data.json\";\nimport Arrow from \"../components/Arrow.vue\";\n\nexport default {\n  name: \"portfolio\",\n  components: {\n    Arrow,\n  },\n  data() {\n    return {\n      categories: data.portfolio.categories,\n      heading: \"Portfolio\",\n      currentFilter: data.portfolio.defaultFilter\n    };\n  },\n  computed: {\n    filteredItems() {\n      const category = this.categories.find(cat => cat.key === this.currentFilter);\n      return category ? category.items : [];\n    }\n  },\n  methods: {\n    setFilter(event) {\n      this.currentFilter = event.target.dataset.filter;\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\">\n  .award-card, .activity-card {\n    background: #222;\n    padding: 15px;\n    border-radius: 5px;\n    margin-bottom: 10px;\n  }\n  .award-card h3, .activity-card h3 {\n    color: #fff;\n  }\n  .award-card p, .activity-card p {\n    color: #ccc;\n  }\n</style>\n"],"mappings":";;;AAuDA,OAAAA,IAAA;AACA,OAAAC,KAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF;EACA;EACAD,KAAA;IACA;MACAI,UAAA,EAAAJ,IA
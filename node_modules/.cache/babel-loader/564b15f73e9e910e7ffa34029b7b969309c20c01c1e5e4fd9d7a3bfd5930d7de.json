aleVisible;\n    },\n    gpaPercentage(gpa) {\n      return (gpa.current / gpa.max * 100).toFixed(2);\n    }\n  }\n};","map":{"version":3,"names":["name","data","filterMode","gpaSystem","scaleVisible","currentScale","Percentage","education","institution","degree","startDate","endDate","description","skills","courses","logo","type","countryFlag","gpa","current","max","computed","filteredEducation","filter","item","methods","setFilter","toggleGpaSystem","systems","currentIndex","indexOf","length","toggleScale","gpaPercentage","toFixed"],"sources":["src/components/EducationSection.vue"],"sourcesContent":["<template>\n  <section id=\"education\" class=\"light-section\">\n    <div class=\"container\">\n      <h1 class=\"section-header\">Education</h1>\n\n      <!-- Filter Buttons -->\n      <div class=\"view-selector\">\n        <button :class=\"{ active: filterMode === 'all' }\" @click=\"setFilter('all')\">\n          All\n        </button>\n        <button :class=\"{ active: filterMode === 'master' }\" @click=\"setFilter('master')\">\n          Masters\n        </button>\n        <button :class=\"{ active: filterMode === 'bachelor' }\" @click=\"setFilter('bachelor')\">\n          Bachelor\n        </button>\n        <button :class=\"{ active: filterMode === 'exchange' }\" @click=\"setFilter('exchange')\">\n          Exchange Semester\n        </button>\n      </div>\n\n      <div class=\"education-list\">\n        <div\n          v-for=\"(item, index) in filteredEducation\"\n          :key=\"index\"\n          class=\"education-item\"\n          :class=\"item.type\"\n        >\n          <div class=\"university-logo\">\n            <img :src=\"item.logo\" :alt=\"item.institution + ' logo'\" />\n          </div>\n          <div class=\"education-details\">\n            <h2 class=\"institution\">\n              <span class=\"flag-icon\" :class=\"'flag-icon-' + item.countryFlag\"></span>\n              {{ item.institution }}\n            </h2>\n            <p class=\"degree\">\n              {{ item.degree }}\n              <span class=\"dates\">({{ item.startDate }} - {{ item.endDate }})</span>\n            </p>\n            <p class=\"gpa\">\n              GPA: {{ item.gpa.current }} / {{ item.gpa.max }} ({{ gpaPercentage(item.gpa) }}%)\n              <button class=\"gpa-switcher\" @click=\"toggleGpaSystem\">\n                {{ gpaSystem }}\n              </button>\n              <button class=\"show-scale\" @click=\"toggleScale\">Show Scale</button>\n            </p>\n            <div v-if=\"scaleVisible\" class=\"scale-table\">\n              <table>\n                <thead>\n                  <tr>\n                    <th>System</th>\n                    <th>Grade</th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"(grade, system) in currentScale\" :key=\"system\">\n                    <td>{{ system }}</td>\n                    <td>{{ grade }}</td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n            <p class=\"description\">{{ item.description }}</p>\n\n            <h3>Skills:</h3>\n            <ul class=\"skills\">\n              <li v-for=\"(skill, i) in item.skills\" :key=\"i\">{{ skill }}</li>\n            </ul>\n\n            <h3>Important Courses:</h3>\n            <ul class=\"courses\">\n              <li v-for=\"(course, i) in item.courses\" :key=\"i\">{{ course }}</li>\n            </ul>\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n\n\n<script>\nexport default {\n  name: \"EducationSection\",\n  data() {\n    return {\n      filterMode: \"all\", // 'all', 'master', 'bachelor', 'exchange'\n      gpaSystem: \"European (1-5)\", // Current GPA system displayed\n      scaleVisible: false, // Toggle for displaying scale table\n      currentScale: {\n        \"European (1-5)\": \"1.0 - 5.0\",\n        Percentage: \"90% - 100%\",\n        \"Letter Grade\": \"A+\",\n  
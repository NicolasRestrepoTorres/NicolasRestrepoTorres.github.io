{"ast":null,"code":"import data from \"@/data/data.js\";\nimport Arrow from \"../components/Arrow.vue\";\nimport Contact from \"../components/Contact.vue\";\nexport default {\n  name: \"AboutContact\",\n  components: {\n    Arrow,\n    Contact\n  },\n  data() {\n    return {\n      about: data.data.about,\n      name: data.data.main.name.first,\n      facts: data.data.about.facts,\n      heading: data.data.main.headings.about,\n      contact: data.data.contact,\n      contactHeading: data.data.main.headings.contact,\n      social: data.data.contact.social\n    };\n  }\n};","map":{"version":3,"names":["data","Arrow","Contact","name","components","about","main","first","facts","heading","headings","contact","contactHeading","social"],"sources":["src/components/About.vue"],"sourcesContent":["<template>\n  <section id=\"about\" class=\"dark-section\">\n    <div class=\"container-fluid\">\n      <h1 class=\"section-header\">{{ heading }}</h1>\n      <h2 class=\"tagline\" v-if=\"about.tagline !== ''\">{{ about.tagline }}</h2>\n      <div class=\"row\">\n        \n        <!-- Profile Picture with Resume Download Button -->\n        <div class=\"col-sm-12 col-md-4 flex-col\" data-aos=\"fade-right\" data-aos-duration=\"1000\">\n          <img class=\"profile-pic\" src=\"../assets/images/profile-pic.jpeg\" alt=\"profile picture\" />\n          <a :href=\"contact.resumeLink\" target=\"_blank\">\n            <button class=\"btn btn-resume mt-3\">\n              <i class=\"far fa-file-pdf\"></i> Resum√©\n            </button>\n          </a>\n        </div>\n\n        <!-- About Text -->\n        <div class=\"col-sm-12 col-md-4 flex-col\" data-aos=\"fade-right\" data-aos-duration=\"1000\">\n          <p id=\"about-content\">\n            <span id=\"greeting\">I'm {{ name }}! </span>\n            <span id=\"bio\" v-html=\"about.bio\"></span>\n          </p>\n        </div>\n\n        <!-- Contact Section Moved Here -->\n        <div class=\"col-sm-12 col-md-4 flex-col\" data-aos=\"fade-right\" data-aos-duration=\"1000\">\n          <h2 class=\"section-header\">{{ contactHeading }}</h2>\n          <ul class=\"list-group list-group-flush\">\n            <li v-if=\"contact.city\" class=\"list-group-item\">\n              <h3 class=\"d-inline\">\n                <i class=\"fas fa-map-marker-alt\"></i> Location:\n              </h3>\n              <span>&nbsp; {{ contact.city }}</span>\n            </li>\n\n            <li v-if=\"contact.email\" class=\"list-group-item\">\n              <h3 class=\"d-inline\">\n                <i class=\"fa fa-envelope\"></i> Email:\n              </h3>\n              <a :href=\"'mailto:' + contact.email\">{{ contact.email }}</a>\n            </li>\n\n            <li v-if=\"contact.phone\" class=\"list-group-item\">\n              <h3 class=\"d-inline\">\n                <i class=\"fas fa-phone\"></i> Phone:\n              </h3>\n              <span>{{ contact.phone }}</span>\n            </li>\n\n            <li class=\"list-group-item\">\n              <h3 class=\"d-inline\">\n    \n              </h3>\n              <div v-for=\"(item, i) in social\" :key=\"i\" class=\"social-item\">\n                <a :href=\"item.url\" target=\"_blank\">\n                  <i :class=\"'fa-lg ' + item.faClass\"></i>\n                </a>\n              </div>\n            </li>\n          </ul>\n        </div>\n\n      </div>\n    </div>\n\n    <Arrow />\n  </section>\n</template>\n\n<script>\nimport data from \"@/data/data.js\";\nimport Arrow from \"../components/Arrow.vue\";\nimport Contact from \"../components/Contact.vue\";\n\nexport default {\n  name: \"AboutContact\",\n  components: {\n    Arrow,\n    Contact,\n  },\n  data() {\n    return {\n      about: data.data.about,\n      name: data.data.main.name.first,\n      facts: data.data.about.facts,\n      heading: data.data.main.headings.about,\n      contact: data.data.contact,\n      contactHeading: data.data.main.headings.contact,\n      social: data.data.contact.social,\n\n    };\n  },\n};\n</script>\n\n<style lang=\"scss\">\n#about {\n  text-align: center;\n\n  .profile-pic {\n    width: 100%;\n    max-width: 400px;\n    height: auto;\n    border-radius: 20px;\n    border: 4px solid rgba(255, 255, 255, 0.8);\n    box-shadow: 0px 4px 15px rgba(0, 0, 0, 0.2);\n    transition: transform 0.3s ease-in-out, box-shadow 0.3s ease-in-out;\n\n\n    // Make it responsive border-radius: 50%;\n\n\n    &:hover {\n      transform: scale(1.05);\n      box-shadow: 0px 8px 20px rgba(0, 0, 0, 0.3);\n    }\n  }\n\n  #about-content {\n    font-size: 1.1rem;\n    line-height: 1.6;\n    color: #f0f0f0;\n  }\n\n  .list-group-item {\n    background: rgba(255, 255, 255, 0.1);\n    border-radius: 8px;\n    padding: 10px 15px;\n    margin: 5px 0;\n    color: #ddd;\n    font-size: 1rem;\n    transition: background 0.3s ease-in-out;\n\n    &:hover {\n      background: rgba(255, 255, 255, 0.2);\n    }\n  }\n}\n\n#contact {\n  .contact-info {\n    text-align: left;\n  }\n\n  .list-group-item a {\n    color: #007bff;\n    text-decoration: none;\n    font-weight: bold;\n  }\n\n  .list-group-item a:hover {\n    text-decoration: underline;\n  }\n\n  .contact-form {\n    background: rgba(255, 255, 255, 0.9);\n    padding: 20px;\n    border-radius: 10px;\n    box-shadow: 0px 4px 15px rgba(0, 0, 0, 0.2);\n  }\n}\n</style>\n"],"mappings":"AAuEA,OAAAA,IAAA;AACA,OAAAC,KAAA;AACA,OAAAC,OAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAH,KAAA;IACAC;EACA;EACAF,KAAA;IACA;MACAK,KAAA,EAAAL,IAAA,CAAAA,IAAA,CAAAK,KAAA;MACAF,IAAA,EAAAH,IAAA,CAAAA,IAAA,CAAAM,IAAA,CAAAH,IAAA,CAAAI,KAAA;MACAC,KAAA,EAAAR,IAAA,CAAAA,IAAA,CAAAK,KAAA,CAAAG,KAAA;MACAC,OAAA,EAAAT,IAAA,CAAAA,IAAA,CAAAM,IAAA,CAAAI,QAAA,CAAAL,KAAA;MACAM,OAAA,EAAAX,IAAA,CAAAA,IAAA,CAAAW,OAAA;MACAC,cAAA,EAAAZ,IAAA,CAAAA,IAAA,CAAAM,IAAA,CAAAI,QAAA,CAAAC,OAAA;MACAE,MAAA,EAAAb,IAAA,CAAAA,IAAA,CAAAW,OAAA,CAAAE;IAEA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}